      @SourceId INT ,
      @Person TTPersonTypeAndId READONLY
    )
RETURNS TABLE
AS RETURN
    SELECT  * ,
            dbo.FNIsPersonCanOperateStep(LastStepId, @Person) AS IsCanOperate
    FROM    dbo.VFlow
    WHERE   FlowTemplateId = ISNULL(@FlowTemplateId, FlowTemplateId)
            AND SourceId = ISNULL(@SourceId, SourceId);
